<?xml version="1.0" encoding="UTF-8"?>   
<selector xmlns:android="http://schemas.android.com/apk/res/android">    
    <!-- Non focused states     -->
    <item android:state_focused="false" android:state_selected="false" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/video_seqence_play_normal" />   
    <item android:state_focused="false" android:state_selected="true" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/video_seqence_play_normal" />   
    <!--  Focused states -->   
    <item android:state_focused="true" android:state_selected="false" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/video_seqence_play_focus" />   
    <item android:state_focused="true" android:state_selected="true" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/video_seqence_play_focus" />    
    <!--Pressed   -->
    <item android:state_pressed="true" android:state_enabled="true" android:drawable="@drawable/video_seqence_play_click" />
    
    <!--disable   -->
    <item android:state_enabled="false" android:drawable="@drawable/video_seqence_play_disable" />      
    
     
  
</selector> 
